{
  "name": "spawnkit",
  "author": "thelastofinusa",
  "description": "A CLI tool to scaffold web3 projects with ease.",
  "version": "1.3.2",
  "license": "MIT",
  "main": "./dist/spawnkit.js",
  "module": "./dist/spawnkit.mjs",
  "types": "./dist/spawnkit.d.ts",
  "bin": {
    "spawnkit": "./dist/spawnkit.mjs"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "dev": "tsx src/spawnkit.ts",
    "watch:dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "pubcli": "npm version patch --no-git-tag-version && npm publish"
  },
  "homepage": "https://spawndapp.vercel.app",
  "bugs": {
    "url": "https://github.com/spawnkit/spawnkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spawnkit/spawnkit.git"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "chalk-animation": "^2.0.3",
    "degit": "^2.8.4",
    "execa": "^8.0.1",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "inquirer": "^13.0.2",
    "nanospinner": "^1.2.2",
    "prompts": "^2.4.2",
    "which-pm-runs": "^1.1.0",
    "zod": "^4.1.13"
  },
  "keywords": [
    "cli",
    "project-generator",
    "scaffolding",
    "template-generator",
    "create-tool",
    "create-app",
    "scaffold",
    "starter-kit",
    "web3",
    "blockchain",
    "ethereum",
    "solidity",
    "hardhat",
    "smart-contracts",
    "dapp",
    "dapp-starter",
    "web3-starter",
    "web3-cli",
    "nextjs",
    "react",
    "frontend",
    "fullstack",
    "spawnkit",
    "dapp-generator",
    "project-setup",
    "codegen",
    "dev-tool"
  ],
  "devDependencies": {
    "@types/chalk-animation": "^1.6.3",
    "@types/degit": "^2.8.6",
    "@types/node": "^25.0.1",
    "@types/prompts": "^2.4.9",
    "@types/which-pm-runs": "^1.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
